<link rel="stylesheet" type="text/css" href="./css/frontPage.css" />
<section class="login">
    <svg
        xmlns="http://www.w3.org/2000/svg"
        xmlns:xlink="http://www.w3.org/1999/xlink"
        width="155"
        height="155"
        viewBox="0 0 155 155"
        fill="none"
    >
        <rect width="155" height="155" fill="url(#pattern0_342_110)" />
        <defs>
            <pattern
                id="pattern0_342_110"
                patternContentUnits="objectBoundingBox"
                width="1"
                height="1"
            >
                <use xlink:href="#image0_342_110" transform="scale(0.01)" />
            </pattern>
            <image
                id="image0_342_110"
                width="100"
                height="100"
                preserveAspectRatio="none"
                xlink:href="data:image/png;base64,"
            />
        </defs>
    </svg>

    <form action="/login" method="post" id="login">
        <P>{{message}}</P>
        <input
            type="email"
            name="email"
            id="email"
            placeholder="Email "
            label
        />
        <input
            type="text"
            id="password"
            placeholder="Kodeord"
            name="password"
        />
        <a href="forgotPassword">Glemt kodeord</a>
        <button class="submit-button" type="submit">login</button>
        <button class="kontakt" type="submit">kontakt</button>
    </form>

</section>